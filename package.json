{
  "name": "@nuxt-modules/algolia",
  "version": "0.4.0",
  "description": "Algolia module for Nuxt 3",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuxt-modules/algolia.git"
  },
  "author": "Jakub Andrzejewski",
  "contributors": [
    "Baroshem <jakub.andrzejewski.dev@gmail.com>"
  ],
  "license": "MIT",
  "exports": {
    ".": {
      "require": "./dist/module.cjs",
      "import": "./dist/module.mjs"
    }
  },
  "main": "dist/module.mjs",
  "module": "dist/module.mjs",
  "types": "dist/module.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "nuxt dev example",
    "docs": "cd docs && yarn dev",
    "lint": "eslint --ext .js,.ts,.vue .",
    "prepublishOnly": "yarn build",
    "release": "yarn lint && standard-version && git push --follow-tags"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config-typescript": "^8.0.0",
    "eslint": "^8.4.1",
    "nuxt3": "^3.0.0-27388331.8ea6b65",
    "standard-version": "^9.3.2",
    "typescript": "^4.5.4",
    "unbuild": "0.6.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "algoliasearch": "^4.11.0",
    "metadata-scraper": "^0.2.49",
    "metascraper": "^5.25.7"
  },
  "bugs": {
    "url": "https://github.com/nuxt-modules/algolia/issues"
  },
  "homepage": "https://github.com/nuxt-modules/algolia#readme",
  "directories": {
    "doc": "docs",
    "example": "example"
  },
  "keywords": [
    "algolia",
    "search",
    "nuxt",
    "module"
  ]
}
